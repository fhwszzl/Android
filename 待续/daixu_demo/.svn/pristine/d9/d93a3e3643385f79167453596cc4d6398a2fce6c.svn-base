    <dimen name="topbar_textSize">20sp</dimen>
    <item name="tag_first" type="id"/>
    <item name="tag_second" type="id"/>
    <string name="action_settings">Settings</string>
    <string name="app_name">ImagePicker</string>
    <string name="hello_world">Hello world!</string>
    <string name="latest_image">最近照片</string>
    <string name="main_back">返回</string>
    <string name="main_cancel">取消</string>
    <string name="main_confirm">确定</string>
